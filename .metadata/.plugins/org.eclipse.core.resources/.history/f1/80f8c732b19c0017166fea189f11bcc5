package trabalho;

public class TesteJogo {
	public static void main(String[] Args) {
		Time time1 = new Time();
		time1.forca = 80;
		time1.nome = "Fortaleza";
		
		Time time2 = new Time();
		time2.forca = 10;
		time2.nome = "Kanal";
		
		Partida partida = new Partida();
		partida.time1 = time1;
		partida.time2 = time2;
		String resultado = partida.Jogar();
		System.out.println(resultado);
	}
}
